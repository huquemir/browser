pipeline{
agent any
stages 
{
stage('Build') 
{
steps{
echo "Building the Code.........."
bat "mvn clean -f Browser_Test/pom.xml"
}
}
stage('Test') 
{
steps{
echo "Testing the Code.........."
bat "mvn test -f Browser_Test/pom.xml"
}
}
stage('Compile') 
{
steps{
echo "Compiling the Project.........."
bat "mvn compile -f Browser_Test/pom.xml"
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}
